{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "strict": false,
      "noImplicitAny": false,
      "skipLibCheck": true,
      "allowUnusedLabels": true,
      
      "emitDecoratorMetadata": true,
      "esModuleInterop": true,
      "allowJs": true,
      "isolatedModules": true,
      "noEmit": true,
      "resolveJsonModule": true
    },
   
  }
  